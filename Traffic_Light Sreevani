void setup()
 {
  pinMode(8, OUTPUT);
 pinMode(10, OUTPUT);
  pinMode(12, OUTPUT);
}

void loop() 
{
  digitalWrite(8,HIGH);
  delay(10);
  digitalWrite(8,LOW);
  delay(10);
  digitalWrite(10,HIGH);
  delay(10);
  digitalWrite(10,LOW);
  delay(10);
  digitalWrite(12,HIGH);
  delay(10);
  digitalWrite(12,LOW);
  delay(10);
}
